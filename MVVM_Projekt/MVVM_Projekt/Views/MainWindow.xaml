<Window x:Class="MVVM_Projekt.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MVVM_Projekt"
        xmlns:vm="clr-namespace:MVVM_Projekt.ViewModels"
        mc:Ignorable="d"
        Title="Totally not Skype :) " Height="400" Width="400">
    <Window.DataContext>
        <vm:MainViewModel/>
    </Window.DataContext>
    <DockPanel>
        <Menu DockPanel.Dock="Top">
            <MenuItem Header="Personen laden" Command="{Binding GetPersonenCommand}"/>
        </Menu>

        <ListBox ItemsSource="{Binding Personen}" />

    </DockPanel>
</Window>
